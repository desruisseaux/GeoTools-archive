<jdo>
	<package name="net.refractions.jpox">

		<class name="Hospital" identity-type="application" objectid-class="javax.jdo.identity.IntIdentity" table="bc_hospitals" detachable="true">
			<field name="gid" primary-key="true"/>
			<field name="id"/>
			<field name="authority"/>
			<field name="name"/>
			<field name="the_geom" persistence-modifier="persistent">
				<extension vendor-name="jpox" key="mapping" value="no-userdata"/>
			</field>
		</class>

	</package>
</jdo>
