<?xml version="1.0" encoding="utf-8"?>
<project>
  <pomVersion>3</pomVersion>
  <extend>${basedir}/../../project.xml</extend>
  <name>geotools-sample-data</name>
  <id>sample-data</id>
  <currentVersion>2.3.x</currentVersion>
  <package>org.geotools</package>
  <shortDescription>sample data module</shortDescription>
  <gumpRepositoryId>geotools</gumpRepositoryId>
  <description>sample data module</description>
  <mailingLists />
  <developers>
    <developer>
      <name>James Macgill</name>
      <id>jmacgill</id>
      <email>jrm33@psu.edu</email>
      <organization>PennState</organization>
      <roles>
        <role>Java Developer</role>
      </roles>
    </developer>
  </developers>
  <dependencies>
    <dependency>
      <artifactId>referencing</artifactId>
      <groupId>gt2</groupId>
      <version>2.3.x</version>
      <!-- We don't really need the referencing module, but we use
           org.geotools.resources.TestData which is defined there. -->
    </dependency>
  </dependencies>
  <contributors></contributors>
  <build>
    <resources>
        <resource>
          <directory>${basedir}/src/</directory>
          <includes>
            <include>org/geotools/test-data/**</include>
          </includes>
        </resource>
    </resources>
  </build>
</project>
